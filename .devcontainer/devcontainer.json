{
  "name": "Python 3",
  "image": "mcr.microsoft.com/vscode/devcontainers/python:3.9",
  "postCreateCommand": "bash ./.devcontainer/postCreate.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.debugpy",
      ]
    }
  },
  "features": {
    "desktop-lite": {
        "password": "vscode",
        "webPort": "6080",
        "vncPort": "5901"
    }
  }
}
